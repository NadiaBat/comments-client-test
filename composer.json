{
  "name": "example/comments-client",
  "license": "MIT",
  "description": "Client for example comments API",
  "require": {
    "php": "8.0",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "ergebnis/http-method": "^2.1",
    "pavelsterba/http-exceptions": "^1.1",
    "kayex/http-codes": "^1.1",
    "thecodingmachine/safe": "^1.3",
    "myclabs/php-enum": "^1.8",
    "ramsey/collection": "^1.2"
  },
  "require-dev": {
    "vimeo/psalm": "^4.8",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "secure-http": false,
    "bin-dir": "vendor/bin",
    "vendor-dir": "vendor",
    "cache-dir": "composer-cache"
  },
  "autoload": {
    "psr-4": {
      "Comments\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Comments\\": "tests/"
    }
  }
}
